CC = gcc
SRC = main.c school.c
OBJ = ${SRC:.c=.o}
NAME = school
RM = rm -f
all: ${NAME}
${NAME}: ${OBJ}
	${CC} ${OBJ} -c -o ${NAME}
%.o: %.c
	${CC} -c -o $@ $<
.PHONY: clean
clean:
	${RM} *~ school
oclean:
	${RM} ${OBJ}
fclean:
	make clean
	make oclean
re:
	 make oclean
	 make all

	